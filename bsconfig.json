{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "reason": {
    "react-jsx": 3
  },
  "suffix": ".js",
  "bs-dependencies": [
    "@rescript/react",
    "bs-css",
    "bs-css-emotion",
    "bs-fetch",
    "bs-express",
    "@ryyppy/rescript-promise"
  ],
  "warnings": {
    "error": "+101"
  },
  "gentypeconfig": {
    "language": "untyped",
    "module": "es6",
    "importPath": "relative",
    "shims": {
      "Js": "Js",
      "ChunkExtractor": "ChunkExtractor"
    },
    "debug": {
      "all": false
    },
    "exportInterfaces": false
  }
}