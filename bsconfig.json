{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "reason": {
    "react-jsx": 3
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "@rescript/react",
    "bs-css",
    "bs-css-emotion",
    "bs-fetch"
  ],
  "gentypeconfig": {
    "language": "typescript"
  },
  "warnings": {
    "error": "+101"
  }
}