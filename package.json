{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently --names www,res,esb -c green.bold,red.bold,blue.bold --kill-others yarn:dev-server yarn:compile:watch yarn:bundle:watch",
    "compile:watch": "NINJA_ANSI_FORCED=1 bsb -make-world -w",
    "bundle": "esbuild src/App.bs.js --outfile=www/app.js --minify --bundle --sourcemap --target=chrome58,firefox57,safari11,edge16 --define:process.env.NODE_ENV='\"development\"'",
    "bundle:watch": "yarn bundle --watch --color=true",
    "dev-server": "PORT=5000 live-server www --host=localhost",
    "build": "bsb -make-world",
    "clean": "bsb -clean-world && rm -rf www/*.js",
    "start": "ts-node-dev webpack-dev-server"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^3.11.3",
    "bs-platform": "*",
    "concurrently": "^6.0.2",
    "esbuild": "^0.11.11",
    "esbuild-loader": "^2.12.0",
    "gentype": "^3.46.0",
    "html-webpack-plugin": "^5.3.1",
    "live-server": "^1.2.1",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.6",
    "tsc": "^1.20150623.0",
    "webpack": "^5.35.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@rescript/react": "^0.10.2",
    "bs-css-emotion": "^2.5.1",
    "bs-fetch": "^0.6.2",
    "webpack-shell-plugin-next": "^2.2.2"
  }
}
